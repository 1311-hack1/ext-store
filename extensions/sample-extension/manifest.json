{
  "manifest_version": 3,
  "name": "Sample Extension",
  "version": "1.0.0",
  "description": "A sample extension to demonstrate the store structure",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Extension Store Team"
} 